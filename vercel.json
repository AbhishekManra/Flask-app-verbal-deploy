{
    "version": 2,
    "builds": [
        {
            "src": "./app.py",
            "use": "@vercel/python"
        }
    ],
    "routes": [
        {
            "src": "/generate",
            "dest": "/app.py",
            "methods": [
                "POST"
            ]
        },
        {
            "src": "/(.*)",
            "dest": "/"
        }
    ]
}