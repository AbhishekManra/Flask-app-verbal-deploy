{
    "version": 2,
    "builds": [
        {
            "src": "./app.py",
            "use": "@vercel/python"
        }
    ],
    "routes": [
        {
            "src": "/generate",
            "dest": "/generate",
            "methods" : ["POST"]
            
        },
        {
            "src": "/(.*)",
            "dest": "/"
        }
    ]
}